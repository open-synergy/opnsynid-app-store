<?xml version="1.0" encoding="UTF-8"?>
<openerp>
    <data>
        <template id="index" inherit_id='point_of_sale.index' name="POS Header">
            <xpath expr="//link[@id='pos-stylesheet']" position="after">
                <link rel="stylesheet" href="/pos_coupons/static/src/css/pos_coupon.css" />
            </xpath>
        </template>

        <template id="wk_assets_pos_coupon" inherit_id="web.assets_common">
