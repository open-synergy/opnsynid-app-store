              <script type="text/javascript" src="/pos_coupons/static/src/js/main.js"></script>
              <script type="text/javascript" src="/pos_coupons/static/src/js/models.js"></script>
              <script type="text/javascript" src="/pos_coupons/static/src/js/widget.js"></script>
          </xpath>
        </template>
     </data>
</openerp>