<openerp>
    <data>
        <template id="indexs" inherit_id='point_of_sale.index' name="POS Header">
            <xpath expr="//link[@id='pos-stylesheet']" position="after">
                <link rel="stylesheet" href="/pos_stock/static/src/css/pos_stock.css" />
            </xpath>
        </template>
        <template id="assets_backends_js" name="pos_stock assests" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
               
               <script type="text/javascript" src="/pos_stock/static/src/js/wk_pos_stock_main.js"></script> 
                 
               <script type="text/javascript" src="/pos_stock/static/src/js/pos_stock_js_file.js"></script>
            </xpath>
        </template>
    </data>
</openerp>
